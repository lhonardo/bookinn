<p id="notice"><%= notice %></p>

<h1>Listing Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Corporatename</th>
      <th>Juridicaldocument</th>
      <th>Extradocument</th>
      <th>Email</th>
      <th>Country</th>
      <th>State</th>
      <th>City</th>
      <th>Address</th>
      <th>Postcode</th>
      <th>Phone</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.corporatename %></td>
        <td><%= company.juridicaldocument %></td>
        <td><%= company.extradocument %></td>
        <td><%= company.email %></td>
        <td><%= company.country %></td>
        <td><%= company.state %></td>
        <td><%= company.city %></td>
        <td><%= company.address %></td>
        <td><%= company.postcode %></td>
        <td><%= company.phone %></td>
        <td><%= company.note %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
